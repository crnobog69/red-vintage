{
  "name": "Red Vintage",
  "type": "dark",
  "colors": {
    "editor.background": "#1c1414",
    "editor.foreground": "#d4c7b9",
    "activityBar.background": "#2a1915",
    "activityBar.foreground": "#d4827c",
    "activityBar.activeBorder": "#a13c32",
    "activityBarBadge.background": "#a13c32",
    "activityBarBadge.foreground": "#e9d9c9",
    "sideBar.background": "#231815",
    "sideBar.foreground": "#c9bab0",
    "sideBarTitle.foreground": "#d4827c",
    "sideBarSectionHeader.background": "#2a1915",
    "sideBarSectionHeader.foreground": "#d4827c",
    "editorGroupHeader.tabsBackground": "#231815",
    "tab.activeBackground": "#2a1915",
    "tab.inactiveBackground": "#1c1414",
    "tab.activeForeground": "#d4827c",
    "tab.inactiveForeground": "#a18d84",
    "titleBar.activeBackground": "#2a1915",
    "titleBar.activeForeground": "#d4827c",
    "statusBar.background": "#2a1915",
    "statusBar.foreground": "#c9bab0",
    "statusBar.noFolderBackground": "#2a1915",
    "statusBar.debuggingBackground": "#a13c32",
    "list.activeSelectionBackground": "#3c2622",
    "list.activeSelectionForeground": "#d4827c",
    "list.inactiveSelectionBackground": "#32211e",
    "list.inactiveSelectionForeground": "#d4827c",
    "list.hoverBackground": "#3c2622",
    "badge.background": "#a13c32",
    "badge.foreground": "#e9d9c9",
    "button.background": "#a13c32",
    "button.foreground": "#e9d9c9",
    "dropdown.background": "#231815",
    "dropdown.foreground": "#c9bab0",
    "input.background": "#231815",
    "input.foreground": "#c9bab0",
    "input.placeholderForeground": "#a18d84",
    "focusBorder": "#a13c32",
    "editor.lineHighlightBackground": "#3c2622",
    "editor.selectionBackground": "#4e342e",
    "editorCursor.foreground": "#d4827c",
    "editorWidget.background": "#231815",
    "editorHoverWidget.background": "#231815",
    "editorHoverWidget.border": "#3c2622",
    "terminal.background": "#1c1414",
    "terminal.foreground": "#d4c7b9",
    "terminal.ansiBlack": "#1c1414",
    "terminal.ansiRed": "#a13c32",
    "terminal.ansiGreen": "#8c7c60",
    "terminal.ansiYellow": "#c9a554",
    "terminal.ansiBlue": "#5c5088",
    "terminal.ansiMagenta": "#976983",
    "terminal.ansiCyan": "#807c99",
    "terminal.ansiWhite": "#d4c7b9",
    "terminal.ansiBrightBlack": "#8a7570",
    "terminal.ansiBrightRed": "#d4827c",
    "terminal.ansiBrightGreen": "#b0a68d",
    "terminal.ansiBrightYellow": "#e6cc94",
    "terminal.ansiBrightBlue": "#9590b3",
    "terminal.ansiBrightMagenta": "#c498b3",
    "terminal.ansiBrightCyan": "#b0acc9",
    "terminal.ansiBrightWhite": "#e9d9c9"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8a7570"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#c498b3"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#c498b3"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#e6cc94"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#d4827c"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#c9a554",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#d4c7b9",
        "background": "#a13c32"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#d4c7b9",
        "background": "#976983"
      }
    },
    {
      "scope": ["keyword", "variable.language.this"],
      "settings": {
        "foreground": "#d4827c"
      }
    },
    {
      "scope": ["markup.inserted.diff"],
      "settings": {
        "foreground": "#8c7c60"
      }
    },
    {
      "scope": ["markup.deleted.diff"],
      "settings": {
        "foreground": "#a13c32"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold.markdown",
      "settings": {
        "foreground": "#d4827c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic.markdown",
      "settings": {
        "foreground": "#c9a554",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.diff.range"],
      "settings": {
        "foreground": "#9590b3"
      }
    },
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#a18d84"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#d4827c"
      }
    },
    {
      "scope": "meta.directive.vue",
      "settings": {
        "foreground": "#9590b3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.property-name.css",
      "settings": {
        "foreground": "#9590b3"
      }
    },
    {
      "scope": "meta.property-value.css",
      "settings": {
        "foreground": "#e6cc94"
      }
    },
    {
      "scope": "meta.tag.other.html",
      "settings": {
        "foreground": "#d4c7b9"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a18d84"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#d4827c"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#c9a554"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#807c99"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#d4827c"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#c9a554"
      }
    },
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#b0acc9"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#c498b3"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call.generic"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#b0acc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "#b0acc9"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9590b3"
      }
    },
    {
      "scope": ["support.function.builtin.python"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.function-call.python", "meta.function-call.generic.python"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.function.builtin.python", "support.function.python"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.python"],
      "settings": {
        "foreground": "#e6cc94",
        "fontStyle": "italic"
      }
    }
  ]
}
